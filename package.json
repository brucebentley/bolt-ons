{
  "name": "bolt-ons",
  "version": "0.0.1",
  "description": "A component framework/toolkit for Angular-based Progressive Web Applications.",
  "keywords": [],
  "homepage": "https://github.com/brucebentley/bolt-ons",
  "author": [
    {
      "name": "Bruce Bentley",
      "email": "brucebentley@me.com",
      "url": "https://github.com/brucebentley/"
    }
  ],
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:aot": "ng build --aot",
    "build:prod": "ng build --aot -prod",
    "build:prod-web": "ng build --aot -prod && npm run sw && cd dist && httpster",
    "clean:dist": "rm -rf dist",
    "e2e": "ng e2e",
    "help": "ng help",
    "lint": "ng lint",
    "prebuild:prod": "npm run clean:dist",
    "serve:alt": "ng serve --port 3333",
    "serve:aot": "ng serve --aot --port 3333",
    "serve:altaot": "ng serve --aot --port 3333",
    "serve:prod": "ng serve --aot -prod --port 3333",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "test": "ng test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brucebentley/bolt-ons.git"
  },
  "bugs": {
    "url": "https://github.com/brucebentley/bolt-ons/issues"
  },
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/platform-webworker": "^4.2.4",
    "@angular/platform-webworker-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "hammerjs": "2.0.8",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
